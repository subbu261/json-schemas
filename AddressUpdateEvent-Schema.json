{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "eventHeader": {
      "type": "object",
      "properties": {
        "sendingSystem": {
          "type": "string",
          "const": "GEKKO"
        },
        "eventId": {
          "type": "string",
          "format": "uuid"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time"
        },
        "eventName": {
          "type": "string",
          "const": "ADDRESSES_UPDATED"
        },
        "downstreamModule": {
          "type": "string",
          "maxLength": 99
        },
        "eventTimestamp": {
          "type": "string",
          "format": "date-time"
        },
        "eventStatus": {
          "type": "string",
          "enum": [
            "SUCCESS",
            "FAILURE"
          ]
        },
        "eventVersion": {
          "type": "string"
        }
      },
      "required": [
        "sendingSystem",
        "eventId",
        "timestamp",
        "eventName",
        "downstreamModule",
        "eventTimestamp",
        "eventStatus",
        "eventVersion"
      ]
    },
    "eventProperties": {
      "type": "object",
      "properties": {
        "transactionId": {
          "type": "string",
          "format": "uuid"
        },
        "orderId": {
          "type": "string",
          "maxLength": 100
        },
        "customerId": {
          "type": "string",
          "maxLength": 100
        },
        "subscriptionIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "hardwareItemIds": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "transactionId",
        "orderId",
        "customerId",
        "subscriptionIds",
        "hardwareItemIds"
      ]
    },
    "eventData": {
      "type": "object",
      "properties": {
        "transactionId": {
          "type": "string",
          "format": "uuid"
        },
        "orderId": {
          "type": "string",
          "maxLength": 100
        },
        "customerId": {
          "type": "string",
          "maxLength": 100
        },
        "items": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "sku": {
                "type": "string",
                "maxLength": 40
              },
              "type": {
                "type": "string",
                "enum": [
                  "SUBS",
                  "BUND",
                  "GOODS"
                ]
              },
              "subType": {
                "type": "string",
                "enum": [
                  "CAREPACK",
                  "HARDWARE",
                  "INK",
                  "PAPER",
                  "LEASE",
                  "PAAS",
                  "CANCELLATION_FEE_REFUND"
                ]
              },
              "additionalIdentifiers": {
                "type": "object",
                "properties": {
                  "sqId": {
                    "type": "string",
                    "maxLength": 10
                  },
                  "contractAccountId": {
                    "type": "string",
                    "maxLength": 12
                  },
                  "salesOrderId": {
                    "type": "string",
                    "maxLength": 10
                  },
                  "subscriptionContractId": {
                    "type": "string",
                    "maxLength": 10
                  },
                  "providerContractId": {
                    "type": "string",
                    "maxLength": 20
                  }
                },
                "required": [
                  "sqId",
                  "contractAccountId",
                  "salesOrderId",
                  "subscriptionContractId",
                  "providerContractId"
                ]
              },
              "timesliceStartDate": {
                "type": "string",
                "format": "date-time"
              },
              "timesliceEndDate": {
                "type": "string",
                "format": "date-time"
              },
              "addresses": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": {
                      "type": "string"
                    },
                    "firstName": {
                      "type": "string"
                    },
                    "lastName": {
                      "type": "string"
                    },
                    "address1": {
                      "type": "string"
                    },
                    "address2": {
                      "type": "string"
                    },
                    "city": {
                      "type": "string"
                    },
                    "stateProvince": {
                      "type": "string"
                    },
                    "countryCode": {
                      "type": "string"
                    },
                    "zipCode": {
                      "type": "string"
                    },
                    "phoneNumber": {
                      "type": "string"
                    },
                    "extension": {
                      "type": "string"
                    },
                    "company": {
                      "type": "string"
                    },
                    "externalAddressID": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "type",
                    "firstName",
                    "lastName",
                    "address1",
                    "address2",
                    "city",
                    "stateProvince",
                    "countryCode",
                    "zipCode",
                    "phoneNumber",
                    "extension",
                    "company",
                    "externalAddressID"
                  ]
                }
              }
            },
            "required": [
              "id",
              "sku",
              "type",
              "subType",
              "additionalIdentifiers",
              "timesliceStartDate",
              "timesliceEndDate",
              "addresses"
            ]
          }
        }
      },
      "required": [
        "transactionId",
        "orderId",
        "customerId",
        "items"
      ]
    }
  },
  "required": [
    "eventHeader",
    "eventProperties",
    "eventData"
  ]
}